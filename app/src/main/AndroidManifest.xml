<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tranvantungit.retrofitproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".FirstActivity">

        </activity>
        <activity
            android:name=".RetrofitActivity"
            android:label="@string/title_activity_retrofit"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NoLibraryActivity"
            android:label="@string/title_activity_no_library"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VolleyActivity"
            android:label="@string/title_activity_volley"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".RxActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>